{
  "name": "next-util-nestjs",
  "version": "v1.1.0",
  "description": "Nestjs template to start",
  "author": "Sagar Variya",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/sv-next-level/next-util-nestjs.git"
  },
  "scripts": {
    "cm": "cz",
    "dev": "clear && nest start --watch",
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lints": "lint-staged",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "preview": "nest build && nest start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release:local": "npx semantic-release --no-ci --dry-run=false",
    "prettier": "prettier .",
    "format": "npm run prettier -- --write --cache && npm run lint:fix",
    "check-formats": "npm run prettier -- --list-different",
    "validate": "npm run check-formats && npm run lint && npm run build",
    "submodule:update": "git submodule update --init --remote",
    "sh:files_to_copy": "chmod +x ./script/files_to_copy.sh && ./script/files_to_copy.sh",
    "sh:files_to_delete": "chmod +x ./script/files_to_delete.sh && ./script/files_to_delete.sh",
    "sh:update_scripts": "chmod +x ./script/update_scripts.sh && ./script/update_scripts.sh",
    "clean": "npm run sh:files_to_delete && npm cache clean --force",
    "preinstall": "npm run submodule:update && npm run sh:files_to_copy",
    "prepare": "husky install",
    "refresh": "yarn clean && yarn",
    "drizzle:generate": "npx drizzle-kit generate --config=src/db/postgres/drizzle/drizzle.config.ts",
    "drizzle:migrate": "npx drizzle-kit migrate --config=src/db/postgres/drizzle/drizzle.config.ts",
    "drizzle:migration": "npm run drizzle:generate && npm run drizzle:migrate"
  },
  "workspaces": [],
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/platform-express": "^10.0.0",
    "axios": "^1.7.2",
    "cache-manager": "^5.6.1",
    "cache-manager-redis-yet": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "drizzle-kit": "^0.24.0",
    "drizzle-orm": "^0.33.0",
    "mongoose": "^8.4.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.0.0",
    "semantic-release": "^24.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
